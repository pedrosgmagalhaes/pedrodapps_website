{
  "hosting": {
    "source": ".",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "frameworksBackend": {
      "region": "us-central1"
    },
    "redirects": [
      { "source": "/members/login", "destination": "/login", "type": 301 }
    ],
    "rewrites": [
      { "source": "**", "destination": "/index.html" }
    ]
  }
}
